import java.util.Scanner;

public class ReverseNumbers {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter N: ");
        int n = sc.nextInt();
        System.out.print("Numbers in reverse: ");
        for (int i = n; i >= 1; i--) {
            System.out.print(i + " ");
        }
    }
}
